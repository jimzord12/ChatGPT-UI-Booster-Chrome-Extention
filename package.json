{
  "name": "chatgpt-ui-booster-chrome-extention",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "tsc && vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "content-script": "tsc src/background/content-script.ts --target ES2020 --module ESNext --outDir extension/webpageModifiers",
    "parcel-build-content": "parcel build extension/content/content-script.ts --dist-dir extension/dist/content && parcel build extension/content/content-script.css --dist-dir extension/dist/content",
    "parcel-dev": "parcel watch src/content-script.js --out-dir dist --out-file content-script.js"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.0",
    "@types/chrome": "^0.0.270",
    "@typescript-eslint/eslint-plugin": "^8.1.0",
    "@typescript-eslint/parser": "^8.1.0",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.9.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-tailwindcss": "^3.17.4",
    "globals": "^15.9.0",
    "parcel": "^2.12.0",
    "postcss": "^8.4.41",
    "tailwindcss": "^3.4.10",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.1.0",
    "vite": "^5.4.1"
  }
}
